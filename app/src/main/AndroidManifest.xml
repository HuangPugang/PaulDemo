                <action android:name="com.igexin.sdk.action.syVq0NWMgYAJFW9JeeJRw9" />
            </intent-filter>
        </receiver>
        <!--个推配置-->
        <meta-data
            android:name="PUSH_APPID"
            android:value="syVq0NWMgYAJFW9JeeJRw9" />
        <!--APPID-->
        <meta-data
            android:name="PUSH_APPKEY"
            android:value="uvlZHxgO8Q7Ow2Zp6qRfm2" />
        <!--APPKEY-->
        <meta-data
