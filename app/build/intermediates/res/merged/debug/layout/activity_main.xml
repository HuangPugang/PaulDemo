                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ImageCache" />

            <Button
                android:id="@+id/stick"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="StickScroll" />

            <Button
                android:id="@+id/clip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Clip" />

