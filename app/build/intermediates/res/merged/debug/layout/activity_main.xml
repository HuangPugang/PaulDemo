<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#00a1ff" />

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/light_control"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="亮度调节" />

            <Button
                android:id="@+id/getphoto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="相册获取图片" />

            <Button
                android:id="@+id/volley"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Volley使用" />

            <Button
                android:id="@+id/getui"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="个推消息推送" />

            <Button
                android:id="@+id/sugar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="sugar orm" />

            <Button
                android:id="@+id/myscrollview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="自定义scrooview" />

            <Button
                android:id="@+id/swiperefresh"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="swiperefresh" />

            <Button
                android:id="@+id/sendBroadcast"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="发送广播" />Ò

            <Button
                android:id="@+id/indicator_"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="indicator" />

            <Button
                android:id="@+id/image_compress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="图片压缩" />

            <Button
                android:id="@+id/wifi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="WIFI操作" />

            <Button
                android:id="@+id/wifi_ap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="WIFI_AP" />

            <Button
                android:id="@+id/socket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="File" />

            <Button
                android:id="@+id/anim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Activity动画" />

            <Button
                android:id="@+id/webview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="webview" />

            <Button
                android:id="@+id/upload"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上传" />

            <Button
                android:id="@+id/elastic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="弹性scrollview" />

            <Button
                android:id="@+id/slidingfinish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="滑动关闭" />

            <Button
                android:id="@+id/js"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="js调用" />

